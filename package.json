{
  "name": "user-event",
  "version": "0.0.0-development",
  "description": "Simulate user events for react-testing-library",
  "keywords": [
    "react-testing-library",
    "dom-testing-library",
    "react",
    "testing"
  ],
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "npm t -- --coverage",
    "codecov": "codecov",
    "build": "babel src -d dist",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "cm": "npx git-cz",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gpx/user-event.git"
  },
  "author": "Giorgio Polvara <polvara@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gpx/user-event/issues"
  },
  "homepage": "https://github.com/Gpx/user-event#readme",
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "all-contributors-cli": "5.4.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "codecov": "3.1.0",
    "husky": "1.1.3",
    "jest": "23.6.0",
    "jest-dom": "2.1.0",
    "lint-staged": "8.0.4",
    "prettier": "1.14.3",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-testing-library": "5.2.3",
    "regenerator-runtime": "0.12.1",
    "semantic-release": "15.10.6",
    "travis-deploy-once": "5.0.9"
  },
  "dependencies": {
    "dom-testing-library": "3.12.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,json}": [
        "prettier --write",
        "git add"
      ],
      "*.md": [
        "prettier --write --parser markdown",
        "git add"
      ]
    },
    "ignore": [
      "package.json",
      "package-lock.json",
      "node_modules/**",
      "dist/**"
    ]
  }
}
