    "@types/react": "^17.0.42",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-local-rules": "^1.1.0",
    "is-ci": "^3.0.1",
    "jest-in-case": "^1.0.2",
    "jest-serializer-ansi": "^1.0.3",
    "kcd-scripts": "^12.1.0",
    "react": "^18.0.0",
